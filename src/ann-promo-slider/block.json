    "blocksPaddings": {
      "type": "string",
      "default": "0px"
    },
    "blocksMargins": {
      "type": "string",
      "default": "0px"
    },
    "blocksGap": {
      "type": "string",
      "default": "10px"
    }

  },
  "textdomain": "uni-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
