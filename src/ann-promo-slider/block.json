{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "uni-blocks/ann-promo-slider",
  "version": "0.1.0",
  "title": "Ann Promo Slider",
  "category": "unitheme",
  "icon": "list-view",
  "description": "slider promotion",
  "example": {},
  "supports": {
	"html": true
  },
  "attributes": {
    "items": {
      "type": "array",
      "default": [
        {
          "img": null,
          "title": null,
          "description": null,
          "btnText": null,
          "label": null
