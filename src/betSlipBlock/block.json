	},
	"passiveTabBackground": {
	  "type": "string",
	  "default": "#f7f7f7"
	},
	"passiveTabColor": {
	  "type": "string",
	  "default": "#686868"
	},
	"activeTabBackground": {
	  "type": "string",
	  "default": "#fff"
	},
	"activeTabColor": {
	  "type": "string",
	  "default": "#222"
