	"bodyBackground": {
	  "type": "string",
	  "default": "#fff"
	},
	"bodyFontColor": {
	  "type": "string",
	  "default": "#686868"
	},
	"footerBackground": {
	  "type": "string",
	  "default": "#fff"
	},
	"footerColor": {
	  "type": "string",
	  "default": "#686868"
	},
	"buttonBackground": {
	  "type": "string",
	  "default": "#f7f7f7"
	},
	"buttonColor": {
	  "type": "string",
	  "default": "#222"
	},
	"stake": {
	  "type": "string",
	  "default": "Stake"
	},
	"odds": {
	  "type": "string",
	  "default": "Odds"
	},
	"possibleWinings": {
	  "type": "string",
	  "default": "Possible Winnings"
	},
	"buttonText": {
	  "type": "string",
	  "default": "Login"
	},
	"currency": {
	  "type": "string",
	  "default": "MWK"
	}

  },
  "textdomain": "uni-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
