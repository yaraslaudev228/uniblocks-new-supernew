{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "uni-blocks/burger-aside",
  "version": "0.1.0",
  "title": "Uni burger menu",
  "category": "unitheme",
  "icon": "schedule",
  "description": "Buttons",
  "example": {},
  "supports": {
	"html": true
  },
  "attributes": {
	"nearIconColor": {
	  "type": "string",
	  "default": "#fff"
	},

	"closeBurgerColor": {
	  "type": "string",
	  "default": "#ddd"
	},

	"asideBackground": {
	  "type": "string",
	  "default": "linear-gradient(#e66465, #9198e5)"
	},
	"align": {
	  "type": "string",
	  "default": "left"
	},
	"burgerImage": {
	  "type": "object",
	  "default": {
		"id": null,
		"url": null
	  }
	},
 	"burgerAsideWidth": {
	  "type": "string",
	  "default": "240px"
	},

	"burgerImageWidth": {
	  "type": "string",
	  "default": "24px"
	},
	"burgerImageHeight": {
	  "type": "string",
	  "default": "24px"
	},
	"burgerMenu": {
	  "type": "string",
	  "default": ""
	},
	"menu": {
	  "type": "array",
	  "default": []
	},
	"menuItemsBorderRadius": {
	  "type": "string",
	  "default": "0"
	}

  },
  "textdomain": "uni-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
