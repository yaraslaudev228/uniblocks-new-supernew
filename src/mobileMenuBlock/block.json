{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "uni-blocks/mobile-trigger",
  "version": "0.1.0",
  "title": "Uni mobile trigger",
  "category": "unitheme",
  "icon": "editor-table",
  "description": "Триггер-иконка для открытия мобильного меню, размещай этот тригер где хочешь, а меню собирай в виджетах",
  "example": {},
  "supports": {
	"html": true
  },
  "attributes": {
	"background": {
	  "type": "string"
	},
	"padding": {
	  "type": "string"
	},
	 "icon": {
	   "type": "object",
	   "default": {}
	 },
	"iconWidth": {
	  "type": "string",
	  "default": "24px"
	},
	"text": {
	  "type": "string",
	  "default": null
	},
	"textColor": {
	  "type": "string",
	  "default": "#ddd"
	},
	"borderRadius": {
	  "type": "string"
	}
  },
  "textdomain": "uni-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
